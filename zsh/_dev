#compdef dev
function _dev_projects() {
	[[ -z "$DEV_PROJECTS_FILE" || ! -f "$DEV_PROJECTS_FILE" ]] && return
	local -a CD_LIST
	CD_LIST=( $(cut -f 1 < "$DEV_PROJECTS_FILE") )
	_describe "Projects" CD_LIST
}
function _dev_pwd() {
	_values "keyword candidates" ${PWD:t}
}
function _dev() {
	_arguments \
	  '(- *)-l[list all projects]' \
	  '(- *)-a[add project]:keyword:_dev_pwd' \
	  '(- *)-d[delete project]:project:_dev_projects' \
	  '(- *)*:project:_dev_projects'
}
